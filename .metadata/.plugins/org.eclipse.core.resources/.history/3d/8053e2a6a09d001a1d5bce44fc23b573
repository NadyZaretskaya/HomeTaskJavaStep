package modul_02;

public class Task08 {
	
	final static int speed = 335;		// m/sec
	final static int time = 27;		// sec
	
		public static void main(String[] args) {
		    double distanse = (time * speed) / 1000.0;
	        
	        System.out.print("Расстояние до места удара молнии " + distanse + " километров");
	}
}
